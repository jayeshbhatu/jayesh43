<script type="text/javascript">
function charge(value)
{
	if(value=="yes")
		{
		
	var charge=	document.getElementById("chargeduty");
		charge.disabled=false;
		}
	else{
		var charge=	document.getElementById("chargeduty");
		charge.disabled=true;
	}
		
}

</script>
