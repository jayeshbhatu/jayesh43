<script type="text/javascript">
$(document).ready(function(){
	 $("#spList").change(function(){
		 var url="<c:url value="/specialitylist/"/>"+ $("#spList option:selected").val();
		  $.ajax({
		mimeType:"appliaction/json",
		   type:"GET",
		   url:url,
		   success:function(data) {
		    $('#subList').empty();
		    
		    if(data!=""){
		    	  $(data).each(function(index, item){
		  		    $("#subList").append("<option value='" + item[0] + "'>" + item[1] + "</option>");
		  		   	});
		    }
		    else{
		    	alert("Unable to retrive cities for selected state " + $("#spList option:selected").val());
				
		    }
		  
		   },
		   error:function(event){
			
   	    alert("Unable1 to retrive cities for selected state " + $("#spList option:selected").val());
			
		   }
	  });})  });
</script>
<script type="text/javascript">
$(document).ready(function()
	{
	//alert($("#spSelected").val());
	 if($("#spSelected").val()!="")
	{
		 var url="<c:url value="/specialitylistselected/"/>"+ $("#spList option:selected").val();
		 //alert(url);
		  $.ajax({
		mimeType:"appliaction/json",
		   type:"GET",
		   url:url,
		   success:function(data) {
		    $('#subList').empty();
		    
		    if(data!=""){
		    	  $(data).each(function(index, item){
		    		  if($('#spSeleted').val()==item.id)
		    	{
		  		    $("#subList").append("<option value='" + item[0] + "' selected='selected'>" + item[1] + "</option>");
		    	}
		    		  else
		    		{
		    			  $("#subList").append("<option value='" + item[0] + "'>" + item[1] + "</option>");
		    	    }
		  		   	});
		    }
		    else{
		    	alert("Unable to retrive cities for selected state " + $("#spList option:selected").val());
				
		    }
		  
		   },
		   error:function(event){
			
   	    alert("Unable1 to retrive cities for selected state " + $("#spList option:selected").val());
			
		   }
	  });}});
</script>


<script type="text/javascript">
function checkAlphabetic(temp)
{
	var re = /^([a-zA-Z ])+$/;
	if(re.test(temp))
	{
		return true;
	}
	else
	{
		return false;	
	}
}
function checkNumber(temp)
{
	var re = /^([0-9])+$/;
	if(re.test(temp))
	{
		return true;
	}
	else
	{
		return false;	
	}
}
function personaldetail_valid() {

	var Firstname = document.getElementById('FirstName');
	var Middlename = document.getElementById('MiddleName');
	var LastName = document.getElementById('LastName');
	var Address1 = document.getElementById('Address1');
	var ContactNumber = document.getElementById('ContactNumber');
	var Pincode = document.getElementById('Pincode');
	var dob = document.getElementById('dob');

	/* FirstName Validation */
	if (Firstname.value == "") {
		//alert("Please Enter First Name");
		document.getElementById('FirstName').placeholder = "Please Enter First Name";
		document.updatePresonalDetail.FirstName.focus();
		return false;
	} else if ((Firstname.value.length < 2)
			|| (Firstname.value.length > 30)) {
		//alert("lenth <2 >30");
		document.getElementById('FirstName').value = "";
		document.getElementById('FirstName').placeholder = "First Name allowed from 2 to 30 characters";
		document.updatePresonalDetail.FirstName.focus();
		return false;
	} else if (checkAlphabetic(Firstname.value) == false) {
		//alert("onl alph");
		document.getElementById('FirstName').value = "";
		document.getElementById('FirstName').placeholder = "Only alphabets are allowed for First Name";
		document.updatePresonalDetail.FirstName.focus();
		return false;
	}

	/* MiddleName Validation */
	if (checkAlphabetic(Middlename.value) == false
			&& Middlename.value.length > 0) {
		//alert("lent >0");
		document.getElementById('MiddleName').value = "";
		document.getElementById('MiddleName').placeholder = "Only alphabets are allowed for middle Name";
		document.updatePresonalDetail.MiddleName.focus();
		return false;
	} else if (Middlename.value.length > 30) {
		//alert("mid <30");
		document.getElementById('MiddleName').value = "";
		document.getElementById('MiddleName').placeholder = "Middle Name allowed upto 30 characters";
		document.updatePresonalDetail.MiddleName.focus();
		return false;
	}

	/* LastName Validation */
	if (LastName.value == "") {
		//alert("enter lname");
		document.getElementById('LastName').value = "";
		document.getElementById('LastName').placeholder = "Please Enter Last Name";
		document.updatePresonalDetail.LastName.focus();
		return false;
	} else if ((LastName.value.length < 2) || (LastName.value.length > 30)) {
		//alert("lenth <2");
		document.getElementById('LastName').value = "";
		document.getElementById('LastName').placeholder = "Last Name allowed from 2 to 30 characters";
		document.updatePresonalDetail.LastName.focus();
		return false;
	} else if (checkAlphabetic(LastName.value) == false) {
		//alert("only alph");
		document.getElementById('LastName').value = "";
		document.getElementById('LastName').placeholder = "Only alphabets are allowed for Last Name";
		document.updatePresonalDetail.LastName.focus();
		return false;
	}

	/* Address1 Validation */
	 if (Address1.value.length < 5) {
		//alert("add lenth <5");
		document.getElementById('Address1').value = "";
		document.getElementById('Address1').placeholder = "Address not allowed less then 5";
		document.updatePresonalDetail.Address1.focus();
		return false;
	}
    else if (Pincode.value.length < 6) {
		//alert(Pincode.value.length);
		document.getElementById('Pincode').value = "";
		document.getElementById('Pincode').placeholder = "Only 6 number pincode are allowed";
		document.updatePresonalDetail.Pincode.focus();
		return false;
	}
	/* DOB Validation */
	//alert(dob.value)
	document.drprflupdate.submit();
	
}
</script>
<script type="text/javascript">
function clinic_valid()
{
		var clinic_nm=document.getElementById('clinicName');
		var clinic_add=document.getElementById('address');
		var clinic_pin=document.getElementById('pin');
		var clinic_country=document.getElementById('country');
		var clinic_charge=document.getElementById('charges');
		
		/* clinic */
		if(clinic_nm.value=="")
		{
			//alert("not null");
			document.getElementById('clinicName').value = "";
			document.getElementById('clinicName').placeholder = "Enter name";
			document.clinicform.address.focus();
			return false;
		}
		else if(checkAlphabetic(clinic_nm.value) == false)
		{
			//alert("Only Alph");
			document.getElementById('clinicName').value = "";
			document.getElementById('clinicName').placeholder = "Only alphabets are allowed";
			document.clinicform.address.focus();
			return false;
		}
		else if ((clinic_nm.value.length < 2) || (clinic_nm.value.length > 30))
		{
			//alert("lenth");
			document.getElementById('clinicName').value = "";
			document.getElementById('clinicName').placeholder = "Name allowed from 2 to 30 characters";
			document.clinicform.address.focus();
			return false;
		}
		
		/* Address */
		if(clinic_add.value=="")
		{
			//alert("not null");
			document.getElementById('address').value = "";
			document.getElementById('address').placeholder = "Must require";
			document.clinicform.state.focus();
			return false;
		}
		
		else if (clinic_add.value.length < 5)
		{
			//alert("lenth");
			document.getElementById('address').value = "";
			document.getElementById('address').placeholder = "Address allowed above 5 characters";
			document.clinicform.state.focus();
			return false;
		}
		
		/* Pincode Validation */
		if (clinic_pin.value == '') {
			//alert("enter pinc");
			document.getElementById('pin').value = "";
			document.getElementById('pin').placeholder = "Please Enter Pincode";
			document.clinicform.country.focus();
			return false;
		} else if (clinic_pin.value != '' && clinic_pin.value.length < 6) {
			//alert(Pincode.value.length);
			document.getElementById('pin').value = "";
			document.getElementById('pin').placeholder = "Only 6 number pincode are allowed";
			document.clinicform.country.focus();
			return false;
		} /* else if (checkNumber(clinic_pin) == false && clinic_pin.value.length > 0) 
		{
			//alert("enter valid");
			document.getElementById('pin').value = "";
			document.getElementById('pin').placeholder = "enter valid pincode";
			document.clinicform.country.focus();
			return false;
		} */
		
		/* country */
		if(clinic_country=='')
		{
			//alert("not null");
			document.getElementById('country').value = "";
			document.getElementById('country').placeholder = "Must required";
			document.clinicform.charges.focus();
			return false;
		}
		else if(checkAlphabetic(clinic_country.value) == false)
		{
			//alert("Only Alph");
			document.getElementById('country').value = "";
			document.getElementById('country').placeholder = "Only alphabetic allowed";
			document.clinicform.charges.focus();
			return false;
		}
		else if ((clinic_country.value.length < 2) || (clinic_country.value.length > 30))
		{
			//alert("lenth");
			document.getElementById('country').value = "";
			document.getElementById('country').placeholder = "Must enter above 2 character";
			document.clinicform.charges.focus();
			return false;
		}
		/* charges */
		if(clinic_charge.value=="")
		{
			//alert("require");
			document.getElementById('charges').value = "";
			document.getElementById('charges').placeholder = "Must required";
			document.clinicform.Pincode.focus();
			return false;
		}
		/* else if(clinic_charge.value.length > 0)
		{
			//alert("only num allow");
			document.getElementById('charges').value = "";
			document.getElementById('charges').placeholder = "Only numbers allowed";
			document.clinicform.Pincode.focus();
			return false;
		} */
		//document.clinicform.submit();
		
		ajaxclinic();
		return true;
		
}

</script>
<script type="text/javascript">

function drprofessinal_valid()
{
	var MCI_NO=document.getElementById('MCI_no');
	
	if(MCI_NO.value=="")
	{
		//alert("Must require");
		document.getElementById('MCI_no').value = "";
		document.getElementById('MCI_no').placeholder = "Must required";
		return false;
	}
	document.drprofessinal.submit();
	return true;
}
</script>


<script type="text/javascript">
function edcation_valid()
{
	var degree=document.getElementById('degree');
	var uni_nm=document.getElementById('university');
	
	if(degree.value=='')
	{
		//alert("Must required");
		document.getElementById('degree').value = "";
		document.getElementById('degree').placeholder = "Must required";
		return false;
	}
	else if(checkAlphabetic(degree.value) == false)
	{
//		alert("only alpha");
		document.getElementById('degree').value = "";
		document.getElementById('degree').placeholder = "Only alphabetic allowed";
		return false;
	}
	
	if(uni_nm.value=='')
	{
		//alert("must requ.");
		document.getElementById('university').value = "";
		document.getElementById('university').placeholder = "Must required";
		return false;
	}
	else if(checkAlphabetic(uni_nm.value) == false)
	{
	//	alert("only alhpa");
		document.getElementById('university').value = "";
		document.getElementById('university').placeholder = "Only alphabetic allowed";
		return false;
	}
	ajaxdegree();
	return true;
}
</script>





<script type="text/javascript">
function ajaxdegree()
{
	

	
	var url="<c:url value="/savedegree/"/>"
	
	$.ajax(
			{
				type: "post",
				url: url,
				cache: false,
				data: 'degreename='+$("#degree").val() + "&universityname="+$("#university").val(),
				success:function(response)
				{
					//alert(response);
					if(response!="")
					{
						$(response)
				    }
				},
				error: function()
				{
					alert("Error in adding");
				}
			});
	
	
	var table=document.getElementById("doctoreducation");
	var row = table.insertRow(table.rows.count);

	// Insert new cells (<td> elements) at the 1st and 2nd position of the "new" <tr> element:
	var cell1 = row.insertCell(0);
	var cell2 = row.insertCell(1);
	
	
	cell1.innerHTML = document.getElementById("degree").value;
	cell2.innerHTML =  document.getElementById("university").value;
	
	document.getElementById("degree").value="";
	 document.getElementById("university").value="";
	
	}



</script>
<script type="text/javascript">
function ajaxclinic()
{
	
	var url="<c:url value="/saveclinic/"/>"
	//alert($("#pin").val());
	$.ajax(
			{
				type: "post",
				url: url,
				cache: false,
				data: 'clinicName='+$("#clinicName").val() + "&address="+$("#address").val()+ "&city="+$("#city").val() + "&state="+$("#state").val() + "&pincode="+$("#pin").val() + "&charges="+$("#charges").val() +"&country="+$("#country").val(),
				success:function(response)
				{
				//	alert(response);
					if(response!="")
					{
						$(response)
				    }
				},
				error: function()
				{
					alert("Error in adding");
				}
			});
	
	var table=document.getElementById("clinictable");
	// Create an empty <tr> element and add it to the 1st position of the table:
	var row = table.insertRow(table.rows.count);

	// Insert new cells (<td> elements) at the 1st and 2nd position of the "new" <tr> element:
	var cell1 = row.insertCell(0);
	var cell2 = row.insertCell(1);
	var cell3 = row.insertCell(2);
	var cell4 = row.insertCell(3);
	var cell5 = row.insertCell(4);
	var cell6 = row.insertCell(5);
	var cell7 = row.insertCell(6);

	// Add some text to the new cells:
	cell1.innerHTML = document.getElementById("clinicName").value;
	cell2.innerHTML =  document.getElementById("address").value;
	cell3.innerHTML =  document.getElementById("city").value;
	cell4.innerHTML =  document.getElementById("state").value;
	cell5.innerHTML =  document.getElementById("country").value;
	cell6.innerHTML =  document.getElementById("pin").value;
	cell7.innerHTML =  document.getElementById("charges").value;
	
	 document.getElementById("clinicName").value="";
	 document.getElementById("address").value="";
	 document.getElementById("city").value="";
		 document.getElementById("state").value="";
			 document.getElementById("country").value="";
				 document.getElementById("pin").value="";
					 document.getElementById("charges").value="";
					 
	 
	
	
	
}
</script>
<script>

$(document).ready(function () {  

$(".dropdown dt a").click(function () {
          $(".dropdown dd ul").slideToggle('fast');
});

      $(".dropdown dd ul li a").on('click', function () {
          $(".dropdown dd ul").hide();
      });

      function getSelectedValue(id) {
           return $("#" + id).find("dt a span.value").html();
      }

      $(document).bind('click', function (e) {
          var $clicked = $(e.target);
          if (!$clicked.parents().hasClass("dropdown")) $(".dropdown dd ul").hide();
      });


      $('.mutliSelect input[type="checkbox"]').on('click', function () {
        
          var title = $(this).closest('.mutliSelect').find('input[type="checkbox"]').val(),
              title = $(this).val() + ",";
        
          if ($(this).is(':checked')) {
              var html = '<span title="' + title + '">' + title + '</span>';
              $('.multiSel').append(html);
              $(".hida").hide();
          } 
          else {
              $('span[title="' + title + '"]').remove();
              var ret = $(".hida");
              $('.dropdown dt a').append(ret);
              
          }
      });});
</script>

